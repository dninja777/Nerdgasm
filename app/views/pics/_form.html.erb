<%= form_for @pic, html: {multipart: true } do |f| %>


<div class="form-group">
    
    
</div>

<div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title, class: "form-control" %>
    
</div>

<div class="form-group">
       <%= f.label :description %>
    <%= f.text_field :description, class: "form-control" %>
    
</div>

<%= f.submit :submit, class: "btn btn-success" %>


<% end %>